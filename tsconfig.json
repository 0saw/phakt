{
    "compilerOptions": {
        "target": "es2021",
        "module": "ESNext",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "skipLibCheck": true,
        "isolatedModules": true,
        "moduleResolution": "Node16"
    }
}
